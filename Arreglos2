import java.io.*;
public class Arreglos2{
public static void main(String args []) throws IOException{

BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

// declaracion de arreglo
int calificaciones[];

// instanciar arreglo
calificaciones = new int[10];
  for( int x=0; x< calificaciones.length; x++) 
  {
  System.out.println("Ingrese la calificacion [] ("+x+"): ");
  calificaciones[x] = Integer.parseInt(br.readLine());
  }

  int contador=0;
  while(contador<calificaciones.length){
  System.out.print("Introduzca la calificacion:");
  num= Integer.parseInt(br.readLine());
  contador++;
  }
 }
}
